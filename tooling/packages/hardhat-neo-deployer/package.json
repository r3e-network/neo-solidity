{
  "name": "@neo-solidity/hardhat-neo-deployer",
  "version": "0.1.0",
  "description": "Hardhat plugin for deploying contracts to Neo blockchain",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "hardhat",
    "hardhat-plugin",
    "neo",
    "blockchain",
    "deployment",
    "neovm"
  ],
  "dependencies": {
    "@neo-solidity/types": "workspace:*",
    "hardhat": "^2.19.0",
    "ethers": "^6.9.0",
    "fs-extra": "^11.2.0",
    "chalk": "^5.3.0",
    "debug": "^4.3.4",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/fs-extra": "^11.0.4",
    "@types/debug": "^4.1.12",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "eslint": "^8.56.0"
  },
  "peerDependencies": {
    "hardhat": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}